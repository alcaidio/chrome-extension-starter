{
  "name": "chrome-extension-starter",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2020 browser module main",
    "affected:lint": "nx affected:lint",
    "start:dev": "nx run-many --target=build --all --configuration development --watch --parallel 5",
    "start:prod": "nx run-many --target=build --all --configuration production --watch --parallel 5",
    "build:dev": "nx run-many --target=build --all --configuration development --parallel 5",
    "build:prod": "nx run-many --target=build --all --configuration production --parallel 5"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.12",
    "@angular/common": "14.2.12",
    "@angular/compiler": "14.2.12",
    "@angular/core": "14.2.12",
    "@angular/forms": "14.2.12",
    "@angular/platform-browser": "14.2.12",
    "@angular/platform-browser-dynamic": "14.2.12",
    "@angular/router": "14.2.12",
    "core-js": "^3.6.5",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular-devkit/core": "^15.2.0",
    "@angular-devkit/schematics": "~14.2.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "14.2.12",
    "@angular/language-service": "14.2.12",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nrwl/angular": "15.8.0",
    "@nrwl/cli": "15.8.0",
    "@nrwl/eslint-plugin-nx": "15.8.0",
    "@nrwl/jest": "15.8.0",
    "@nrwl/js": "15.8.0",
    "@nrwl/linter": "15.8.0",
    "@nrwl/web": "15.8.0",
    "@nrwl/workspace": "15.8.0",
    "@schematics/angular": "~14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/chrome": "^0.0.218",
    "@types/jest": "29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "commitizen": "^4.2.4",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "29.4.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-preset-angular": "13.0.0",
    "lint-staged": "^12.1.2",
    "nx": "15.8.0",
    "prettier": "2.8.4",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-rational-order": "^1.0.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
